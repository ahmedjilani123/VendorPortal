<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		
		noDataText="No Products Found"
		title="Select Country"
		search=".DialogSearchField"
		confirm=".onDialogSelectFieldClose"
		cancel=".onDialogCancel"
        id="CSCIdTable"
		items="{
			path: 'CSCData>/'
		}">
		<StandardListItem title="{CSCData>name}"  type="Active" />
	</SelectDialog>
</core:FragmentDefinition>